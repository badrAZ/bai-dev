[
    {
        "key": "ctrl+k ctrl+c",
        "command": "-editor.action.addCommentLine",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+r",
        "command": "workbench.action.gotoSymbol"
    },
    {
        "key": "ctrl+shift+o",
        "command": "-workbench.action.gotoSymbol"
    },
    {
        "key": "ctrl+shift+3",
        "command": "workbench.action.terminal.split",
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+shift+5",
        "command": "-workbench.action.terminal.split",
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+shift+2",
        "command": "workbench.action.terminal.focus"
    },
    {
        "key": "ctrl+shift+1",
        "command": "workbench.action.terminal.toggleTerminal"
    },
    {
        "key": "cmd+[Period]",
        "command": "editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+[Period]",
        "command": "-editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+numpad_divide",
        "command": "editor.action.blockComment",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+a",
        "command": "-editor.action.blockComment",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+alt+i",
        "command": "eslint.executeAutofix"
    },
    {
        "key": "f9",
        "command": "editor.action.sortLinesAscending"
    },
    {
        "key": "ctrl+u",
        "command": "editor.action.transformToUppercase"
    },
    {
        "key": "ctrl+numpad1",
        "command": "gitlens.diffWithBranch"
    },
    {
        "key": "shift+alt+w w",
        "command": "-console.log.wrap.string",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+w down",
        "command": "-console.log.wrap.string.down",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+alt+w down",
        "command": "-console.log.wrap.down.input",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+l",
        "command": "console.log.wrap.down.prefix",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+w ctrl+alt+down",
        "command": "-console.log.wrap.down.prefix",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+e",
        "command": "-workbench.action.quickOpenNavigatePreviousInFilePicker",
        "when": "inFilesPicker && inQuickOpen"
    },
    {
        "key": "ctrl+e",
        "command": "-workbench.action.quickOpenNavigateNextInFilePicker",
        "when": "inFilesPicker && inQuickOpen"
    },
    {
        "key": "ctrl+e",
        "command": "-workbench.action.quickOpen"
    },
    {
        "key": "ctrl+e",
        "command": "workbench.view.explorer"
    },
    {
        "key": "ctrl+shift+e",
        "command": "-workbench.view.explorer"
    },
    {
        "key": "ctrl+shift+[Backquote]",
        "command": "workbench.action.closePanel"
    },
    {
        "key": "ctrl+shift+numpad_subtract",
        "command": "gitlens.openCommitInRemote"
    },
    {
        "key": "ctrl+shift+numpad_multiply",
        "command": "gitlens.toggleFileBlame",
        "when": "editorTextFocus && config.gitlens.keymap == 'chorded' && gitlens:activeFileStatus =~ /blameable/"
    },
    {
        "key": "ctrl+shift+g b",
        "command": "-gitlens.toggleFileBlame",
        "when": "editorTextFocus && config.gitlens.keymap == 'chorded' && gitlens:activeFileStatus =~ /blameable/"
    },
    {
        "key": "cmd+[Period]",
        "command": "-breadcrumbs.focus",
        "when": "breadcrumbsPossible && breadcrumbsVisible"
    },
    {
        "key": "cmd+k cmd+c",
        "command": "-editor.action.addCommentLine",
        "when": "editorTextFocus && !editorReadonly"
    }
 ]
